{
  "name": "n8n-nodes-coupang-partners",
  "version": "0.1.0",
  "description": "n8n community node for Coupang Partners Open API",
  "keywords": [
    "n8n-community-node-package",
    "coupang",
    "coupang partners",
    "affiliate",
    "korean e-commerce",
    "coupang api"
  ],
  "license": "MIT",
  "author": {
    "name": "Aiden Ahn",
    "email": "seungwonme@kakao.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seungwonme/n8n-nodes-coupang-partners.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CoupangPartnersApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/coupang-partners/CoupangPartners.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/parser": "~8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
